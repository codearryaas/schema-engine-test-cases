{
    "name": "toolpress/schema-engine-test-cases",
    "description": "Automated testing plugin for Schema Engine (free and pro)",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "brain/monkey": "^2.6",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "SchemaEngineTestCases\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SchemaEngineTestCases\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    }
}
